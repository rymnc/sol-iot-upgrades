  "license": "MIT",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.10.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "ipfs-http-client": "^50.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "fastify-tsconfig": "^1.0.1",
    "ipfs-core-types": "^0.5.0",
    "tap": "^14.11.0",
    "typescript": "^4.1.3"
  }
